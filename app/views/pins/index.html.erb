<div class="panel panel-default">

<div class="jumbotron"> 
<h1>Witaj w mojej aplikacji!</h1>
<p>Niebawem rzuci wszystkich na kolana...</p>
<%= link_to 'Strona główna', root_path, class: 'btn btn-primary' %>
<%= link_to 'O mnie', home_about_path, class: 'btn btn-default' %> 
</div>

<div class="panel-body">

<div id="pins" class="transitions-enabled">
    <% @pins.each do |pin| %>
      <div class="box">
        <div class="panel panel-default">
          <%= link_to image_tag(pin.image.url(:medium)), pin %>
            <br />
          <div class="panel-body">  
            <%= pin.title %>
          </div>
            <% if pin.user == current_user %>
              <div class="panel-footer">
                <%= link_to 'Edit', edit_pin_path(pin) %><br />
                <%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' } %>
              </div>
            <% end %>
        </div>
      </div>
    <% end %>
  </div>
<br>

</div>
<div class="panel-footer">
<%= link_to 'New Pin', new_pin_path %>
  <div class="center>
    <%= will_paginate @pins, renderer: BootstrapPagination::Rails %>
  </div>
</div>
</div>
